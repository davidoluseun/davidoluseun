{
  "name": "davidoluseun",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "server": "concurrently \"npx tsc --watch\" \"nodemon -q build/index.js --ignore frontend/\"",
    "build": "npx tsc",
    "client": "npm start --prefix frontend",
    "clientbuild": "npm run build --prefix frontend",
    "dev": "concurrently \"npm run server\"  \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npx tsc && npm install --prefix frontend && npm run build --prefix frontend"
  },
  "engines": {
    "node": "14.17.5"
  },
  "author": "David Oluseun",
  "license": "MIT",
  "dependencies": {
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.14",
    "@types/joi-phone-number": "^5.0.4",
    "@types/node": "^18.7.19",
    "@types/nodemailer": "^6.4.6",
    "config": "^3.3.7",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "googleapis": "^107.0.0",
    "joi": "^17.6.0",
    "joi-objectid": "^4.0.2",
    "joi-phone-number": "^5.1.1",
    "mongoose": "^6.5.4",
    "nodemailer": "^6.7.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19"
  }
}
